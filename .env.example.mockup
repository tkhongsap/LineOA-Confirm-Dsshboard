# Mockup Mode Configuration
# This mode uses deterministic mock data for demos and UI/UX validation

# Operation mode (required)
MODE=MOCKUP

# Data retention period in days
RETENTION_DAYS=30

# Seed for deterministic random data generation
# Use the same seed to get consistent mock data across instances
MOCK_DATA_SEED=12345

# Server port
PORT=5000

# Log level (debug | info | warn | error)
LOG_LEVEL=debug